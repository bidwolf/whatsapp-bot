{
  "name": "whatsapp-api",
  "version": "3.0.8",
  "description": "whatsapp-api is builton top of Baileys-MD.",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha tests/*.test.js --exit",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "git@github.com:bidwolf/whatsapp-bot",
  "authors": [
    "Henrique de Paula Rodrigues <tec.henriquedepaula@gmail.com"
  ],
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@whiskeysockets/baileys": "^6.7.8",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-exception-handler": "^1.3.111",
    "link-preview-js": "^3.0.12",
    "mongodb": "^4.17.2",
    "mongoose": "^6.13.3",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.30.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "mocha": "^10.7.3",
    "nodemon": "^2.0.22",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4"
  },
  "packageManager": "yarn@4.3.1+sha512.af78262d7d125afbfeed740602ace8c5e4405cd7f4735c08feb327286b2fdb2390fbca01589bfd1f50b1240548b74806767f5a063c94b67e431aabd0d86f7774"
}
