{
  "name": "whatsapp-api",
  "version": "3.0.8",
  "description": "whatsapp-api is built on top of Baileys-MD.",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/server.js",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha tests/*.test.js --exit",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "git@github.com:bidwolf/whatsapp-bot",
  "authors": [
    "Henrique de Paula Rodrigues <tec.henriquedepaula@gmail.com"
  ],
  "license": "MIT",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "^6.7.9",
    "axios": "^1.7.7",
    "bull": "^4.16.4",
    "chalk": "^5.3.0",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-exception-handler": "^1.3.111",
    "human-readable": "^0.2.1",
    "jimp": "^1.6.0",
    "link-preview-js": "^3.0.12",
    "moment-timezone": "^0.5.46",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pino": "^8.21.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "mocha": "^10.8.2",
    "nodemon": "^2.0.22",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@4.3.1+sha512.af78262d7d125afbfeed740602ace8c5e4405cd7f4735c08feb327286b2fdb2390fbca01589bfd1f50b1240548b74806767f5a063c94b67e431aabd0d86f7774"
}
